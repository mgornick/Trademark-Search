<h2>Search Results</h2>

<table>
  <tr>
    <th>URL</th>
    <th>Search engine</th>
  </tr>

<% @search_results.each do |search_result| %>
  <tr class="<%= cycle("even", "odd") %>">
    <td><%=h search_result.url %></td>
    <td><%=h search_result.search_engine %></td>
    <td><%= link_to 'Show', search_result %></td>
    <td><%= link_to 'Edit', edit_search_result_path(search_result) %></td>
    <td><%= link_to 'Destroy', search_result, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New search_result', new_search_result_path %>